<%= form_for @customer do |u|%>
  <%= u.label :name%>
  <%= u.text_field :name%>
  <br>
  <%= u.label :phone_number %>
  <%= u.text_field :phone_number%>
  <br>
  <%= u.label :address %>
  <%= u.text_field :address %>
  <br>
  <%= u.hidden_field :company_id, value: current_user.company.id%>
  <%= u.submit%>
<%end%>
